{
  "agents": {
    "chat_agent": {
      "model": "openai/gpt-oss-20b",
      "temperature": 0.9,
      "max_tokens": 1000
    },
    "inventory": {
      "model": "openai/gpt-oss-20b",
      "temperature": 0.8,
      "max_tokens": 800
    },
    "summary_agent": {
      "model": "openai/gpt-oss-20b",
      "temperature": 1.0,
      "max_tokens": 500
    },
    "forecasting": {
      "model": "openai/gpt-oss-20b",
      "temperature": 0.9,
      "max_tokens": 1000
    },
    "ordering": {
      "model": "openai/gpt-oss-20b",
      "temperature": 0.8,
      "max_tokens": 800
    }
  },
  "global_settings": {
    "redis_url": "redis://localhost:6379",
    "query_channel": "dev_query_channel",
    "result_channel_template": "dev_result_channel:{agent_type}",
    "agent_names": ["inventory", "forecasting", "ordering"]
  }
}