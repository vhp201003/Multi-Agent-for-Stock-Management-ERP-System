{
  "agents": {
    "chat_agent": {
      "model": "gpt-4",
      "temperature": 0.5,
      "max_tokens": 3000
    },
    "inventory": {
      "model": "gpt-4",
      "temperature": 0.3,
      "max_tokens": 2000
    },
    "summary_agent": {
      "model": "gpt-4",
      "temperature": 0.4,
      "max_tokens": 1500
    },
    "forecasting": {
      "model": "gpt-4",
      "temperature": 0.2,
      "max_tokens": 2500
    },
    "ordering": {
      "model": "gpt-4",
      "temperature": 0.3,
      "max_tokens": 2000
    }
  },
  "global_settings": {
    "redis_url": "redis://production-redis:6379",
    "query_channel": "prod_query_channel",
    "result_channel_template": "prod_result_channel:{agent_type}",
    "agent_names": ["inventory", "forecasting", "ordering"]
  }
}